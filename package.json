{
  "name": "heroes-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-ng": "ng serve --proxy-config proxy.conf.json --port 7626 --open",
    "start": "WWW=dist/heroes-angular node server.js",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "map": "source-map-explorer dist/heroes-angular/*es2015*.js*",
    "quick": "concurrently \"npm run backend\" \"npm run start-ng\"",
    "cypress": "npx cypress open",
    "e2e": "concurrently \"npm run quick\" \"npm run cypress\"",
    "backend": "json-server --watch db.json --routes routes.json --port 7627 --middlewares ./node_modules/json-server-reset"
  },
  "private": true,
  "dependencies": {

    "@angular/animations": "^9.0.2",
    "@angular/common": "^9.0.2",
    "@angular/compiler": "^9.0.2",
    "@angular/core": "^9.0.2",
    "@angular/forms": "^9.0.2",
    "@angular/platform-browser": "^9.0.2",
    "@angular/platform-browser-dynamic": "^9.0.2",
    "@angular/router": "^9.0.2",
    "@ngrx/data": "^9.0.0-rc.0",
    "@ngrx/effects": "^9.0.0-rc.0",
    "@ngrx/entity": "^9.0.0-rc.0",
    "@ngrx/store": "^9.0.0-rc.0",
    "@ngrx/store-devtools": "^9.0.0-rc.0",
    "bulma": "^0.8.0",
    "core-js": "^3.6.4",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.3",
    "@angular/cli": "^9.0.3",
    "@angular/compiler-cli": "^9.0.2",
    "@angular/language-service": "^9.0.2",
    "@types/jasmine": "^3.5.6",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.7.4",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.1.0",
    "cypress": "^4.0.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.16.1",
    "json-server-reset": "^1.3.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "protractor": "^5.4.3",
    "source-map-explorer": "^2.3.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
